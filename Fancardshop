    let selectedCategory = null;
    let followedCelebrities = new Set();
    let searchQuery = '';
    let searchFilter = 'all';
    let searchSuggestions = [];

    function getFilteredCelebrities() {
      let filtered = celebrities;

      // Apply search query filter
      if (searchQuery) {
        const query = searchQuery.toLowerCase();
        filtered = filtered.filter(celeb => 
          celeb.name.toLowerCase().includes(query) ||
          categories.find(c => c.id === celeb.category).name.toLowerCase().includes(query) ||
          celeb.icon.includes(query) ||
          celeb.fans.toLowerCase().includes(query)
        );
      }

      // Apply category filter from search filters
      if (searchFilter !== 'all') {
        filtered = filtered.filter(c => c.category === searchFilter);
      }

      // Apply category filter from category cards (takes precedence)
      if (selectedCategory) {
        filtered = filtered.filter(c => c.category === selectedCategory);
      }

      return filtered;
    }

    function generateSearchSuggestions(query) {
      if (!query || query.length < 2) return [];
      
      const suggestions = [];
      const queryLower = query.toLowerCase();
      
      // Add celebrity name suggestions
      celebrities.forEach(celeb => {
        if (celeb.name.toLowerCase().includes(queryLower) && suggestions.length < 8) {
          suggestions.push({
            type: 'celebrity',
            name: celeb.name,
            category: categories.find(c => c.id === celeb.category).name,
            icon: celeb.icon,
            id: celeb.id
          });
        }
      });
      
      // Add category suggestions
      categories.forEach(cat => {
        if (cat.name.toLowerCase().includes(queryLower) && suggestions.length < 8) {
          suggestions.push({
            type: 'category',
            name: cat.name,
            category: `${cat.count} celebrities`,
            icon: cat.icon,
            id: cat.id
          });
        }
      });
      
      return suggestions;
    }

    function updateSearchStats() {
      const filtered = getFilteredCelebrities();
      const searchStats = document.getElementById('searchStats');
      
      if (searchQuery || searchFilter !== 'all' || selectedCategory) {
        const totalFans = filtered.reduce((sum, celeb) => {
          const fanCount = parseFloat(celeb.fans.replace('M', '').replace('K', '')) * 
                          (celeb.fans.includes('M') ? 1000000 : celeb.fans.includes('K') ? 1000 : 1);
          return sum + fanCount;
        }, 0);
        
        const avgRating = filtered.length > 0 ? 
          (filtered.reduce((sum, celeb) => sum + parseFloat(celeb.rating), 0) / filtered.length).toFixed(1) : 0;
        
        const formatFans = (count) => {
          if (count >= 1000000000) return (count / 1000000000).toFixed(1) + 'B';
          if (count >= 1000000) return (count / 1000000).toFixed(1) + 'M';
          if (count >= 1000) return (count / 1000).toFixed(1) + 'K';
          return count.toString();
        };
        
        document.getElementById('totalResults').textContent = filtered.length;
        document.getElementById('avgRating').textContent = avgRating;
        document.getElementById('totalFans').textContent = formatFans(totalFans);
        
        searchStats.style.display = 'flex';
      } else {
        searchStats.style.display = 'none';
      }
    }

    function updateSearchResults() {
      const filtered = getFilteredCelebrities();
      const resultsInfo = document.getElementById('searchResultsInfo');
      const categoriesSection = document.getElementById('categoriesSection');
      const celebritiesSectionTitle = document.getElementById('celebritiesSectionTitle');

      if (searchQuery || searchFilter !== 'all') {
        resultsInfo.style.display = 'block';
        
        let resultText = `Found ${filtered.length} result${filtered.length !== 1 ? 's' : ''}`;
        if (searchQuery) {
          resultText += ` for "${searchQuery}"`;
        }
        if (searchFilter !== 'all') {
          const filterCategory = categories.find(c => c.id === searchFilter);
          resultText += ` in ${filterCategory.name}`;
        }
        
        resultsInfo.textContent = resultText;
        categoriesSection.style.display = 'none';
        celebritiesSectionTitle.textContent = 'Search Results';
      } else {
        resultsInfo.style.display = 'none';
        categoriesSection.style.display = 'block';
        celebritiesSectionTitle.textContent = selectedCategory ? 
          `${categories.find(c => c.id === selectedCategory).name} Celebrities` : 
          'Featured Celebrities';
      }

      updateSearchStats();
      renderCelebrities();
    }

    function renderCategories() {
      const grid = document.getElementById('categoriesGrid');
      grid.innerHTML = categories.map(cat => `
        <div class="category-card ${selectedCategory === cat.id ? 'active' : ''}" data-category="${cat.id}">
          <span class="category-icon">${cat.icon}</span>
          <div class="category-name">${cat.name}</div>
          <div class="category-count">${cat.count} Celebrities</div>
        </div>
      `).join('');

      document.querySelectorAll('.category-card').forEach(card => {
        card.addEventListener('click', () => {
          const category = card.dataset.category;
          selectedCategory = selectedCategory === category ? null : category;
          renderCategories();
          updateSearchResults();
        });
      });
    }

    function renderCelebrities() {
      const grid = document.getElementById('celebritiesGrid');
      const filtered = getFilteredCelebrities();

      if (filtered.length === 0) {
        const emptyMessage = searchQuery ? 
          `No celebrities found matching "${searchQuery}"` : 
          'No celebrities found in this category';
        
        grid.innerHTML = `
          <div class="empty-state">
            <div class="empty-state-icon">ğŸ”</div>
            <div class="empty-state-text">${emptyMessage}</div>
          </div>
        `;
        return;
      }

      grid.innerHTML = filtered.map(celeb => {
        const isHighlighted = searchQuery && 
          (celeb.name.toLowerCase().includes(searchQuery.toLowerCase()) ||
           categories.find(c => c.id === celeb.category).name.toLowerCase().includes(searchQuery.toLowerCase()));

        return `
          <div class="celebrity-card ${isHighlighted ? 'highlight' : ''}">
            <div class="celebrity-image">${celeb.icon}</div>
            <div class="celebrity-info">
              <div class="celebrity-name">${celeb.name}</div>
              <div class="celebrity-category">${categories.find(c => c.id === celeb.category).name}</div>
              <div class="celebrity-stats">
                <div class="stat">
                  <div class="stat-value">${celeb.fans}</div>
                  <div class="stat-label">Fans</div>
                </div>
                <div class="stat">
                  <div class="stat-value">${celeb.posts}</div>
                  <div class="stat-label">Posts</div>
                </div>
                <div class="stat">
                  <div class="stat-value">${celeb.rating}</div>
                  <div class="stat-label">Rating</div>
                </div>
              </div>
              <button class="btn-follow ${followedCelebrities.has(celeb.id) ? 'following' : ''}" data-id="${celeb.id}">
                ${followedCelebrities.has(celeb.id) ? 'âœ“ Following' : '+ Follow'}
              </button>
            </div>
          </div>
        `;
      }).join('');

      document.querySelectorAll('.btn-follow').forEach(btn => {
        btn.addEventListener('click', (e) => {
          e.stopPropagation();
          const id = parseInt(btn.dataset.id);
          const celebrity = celebrities.find(c => c.id === id);
          
          if (followedCelebrities.has(id)) {
            followedCelebrities.delete(id);
          } else {
            followedCelebrities.add(id);
            // Open email draft for fan card request
            openCelebrityEmailDraft(celebrity);
          }
          renderCelebrities();
        });
      });
    }

    function renderSearchSuggestions(suggestions) {
      const suggestionsContainer = document.getElementById('searchSuggestions');
      
      if (suggestions.length === 0) {
        suggestionsContainer.classList.remove('active');
        return;
      }
      
      suggestionsContainer.innerHTML = suggestions.map(suggestion => `
        <div class="suggestion-item" data-type="${suggestion.type}" data-id="${suggestion.id}" data-name="${suggestion.name}">
          <span class="suggestion-icon">${suggestion.icon}</span>
          <div class="suggestion-info">
            <div class="suggestion-name">${suggestion.name}</div>
            <div class="suggestion-category">${suggestion.category}</div>
          </div>
        </div>
      `).join('');
      
      suggestionsContainer.classList.add('active');
      
      // Add click handlers for suggestions
      document.querySelectorAll('.suggestion-item').forEach(item => {
        item.addEventListener('click', () => {
          const type = item.dataset.type;
          const name = item.dataset.name;
          const id = item.dataset.id;
          
          if (type === 'celebrity') {
            document.getElementById('searchInput').value = name;
            searchQuery = name;
          } else if (type === 'category') {
            searchFilter = id;
            document.getElementById('searchInput').value = '';
            searchQuery = '';
            updateSearchFilters();
          }
          
          suggestionsContainer.classList.remove('active');
          updateSearchResults();
        });
      });
    }

    function updateSearchFilters() {
      document.querySelectorAll('.search-filter').forEach(filter => {
        const filterValue = filter.dataset.filter;
        if (filterValue === searchFilter) {
          filter.classList.add('active');
        } else {
          filter.classList.remove('active');
        }
      });
    }

    function setupSearch() {
      const searchInput = document.getElementById('searchInput');
      const searchClear = document.getElementById('searchClear');
      const suggestionsContainer = document.getElementById('searchSuggestions');

      // Setup search filters
      document.querySelectorAll('.search-filter').forEach(filter => {
        filter.addEventListener('click', () => {
          const filterValue = filter.dataset.filter;
          
          if (searchFilter === filterValue) {
            searchFilter = 'all';
          } else {
            searchFilter = filterValue;
          }
          
          selectedCategory = null;
          renderCategories();
          updateSearchFilters();
          updateSearchResults();
        });
      });

      searchInput.addEventListener('input', (e) => {
        searchQuery = e.target.value.trim();
        
        if (searchQuery) {
          searchClear.style.display = 'block';
          selectedCategory = null;
          renderCategories();
          
          // Generate and show suggestions
          const suggestions = generateSearchSuggestions(searchQuery);
          renderSearchSuggestions(suggestions);
        } else {
          searchClear.style.display = 'none';
          suggestionsContainer.classList.remove('active');
        }
        
        updateSearchResults();
      });

      searchInput.addEventListener('focus', () => {
        if (searchQuery && searchQuery.length >= 2) {
          const suggestions = generateSearchSuggestions(searchQuery);
          renderSearchSuggestions(suggestions);
        }
      });

      searchInput.addEventListener('blur', (e) => {
        // Delay hiding suggestions to allow clicks
        setTimeout(() => {
          if (!e.relatedTarget || !e.relatedTarget.closest('.search-suggestions')) {
            suggestionsContainer.classList.remove('active');
          }
        }, 200);
      });

      searchClear.addEventListener('click', () => {
        searchInput.value = '';
        searchQuery = '';
        searchClear.style.display = 'none';
        suggestionsContainer.classList.remove('active');
        updateSearchResults();
        searchInput.focus();
      });

      searchInput.addEventListener('keydown', (e) => {
        if (e.key === 'Escape') {
          searchInput.value = '';
          searchQuery = '';
          searchClear.style.display = 'none';
          suggestionsContainer.classList.remove('active');
          updateSearchResults();
        }
      });

      // Close suggestions when clicking outside
      document.addEventListener('click', (e) => {
        if (!e.target.closest('.search-box')) {
          suggestionsContainer.classList.remove('active');
        }
      });
    }

    function showModal(modalId) {
      document.getElementById(modalId).classList.add('active');
    }

    function hideModal(modalId) {
      document.getElementById(modalId).classList.remove('active');
    }

    function openEmailDraft() {
      const subject = encodeURIComponent('Premium Membership Request - Fan Card Access');
      const body = encodeURIComponent(`Hello CelebrityFanCard Team,

I would like to request premium membership access to get exclusive fan cards.

My favorite celebrity is: [Please enter your favorite celebrity name here]

Additional information about me:
- [Please share a few details about yourself]
- [Your interests and why you're a fan]
- [Any other relevant information]

Thank you for considering my request!

Best regards,
[Your Name]`);
      
      const mailtoLink = `mailto:premium@celebrityfancard.com?subject=${subject}&body=${body}`;
      window.open(mailtoLink, '_blank');
    }

    function openCelebrityEmailDraft(celebrity) {
      const categoryName = categories.find(c => c.id === celebrity.category).name;
      const subject = encodeURIComponent(`Fan Card Request - ${celebrity.name} (${categoryName})`);
      const body = encodeURIComponent(`Hello CelebrityFanCard Team,

I would like to request an exclusive fan card for ${celebrity.name}!

Celebrity Details:
- Name: ${celebrity.name}
- Category: ${categoryName}
- Current Fans: ${celebrity.fans}
- Rating: ${celebrity.rating}/5.0

Why I'm a fan of ${celebrity.name}:
- [Please share why you're a fan - their work, achievements, or personal inspiration]
- [Mention your favorite songs/movies/performances/games by this celebrity]
- [Share how long you've been following them]

Personal Information:
- My Name: [Your full name]
- Age: [Your age]
- Location: [Your city/country]
- Social Media: [Your Instagram/Twitter handle if you'd like to share]

Special Requests:
- [Any specific design preferences for the fan card]
- [Favorite photo/era of this celebrity]
- [Any quotes or achievements you'd like featured]

I'm excited to add ${celebrity.name} to my fan card collection and show my support!

Thank you for considering my request.

Best regards,
[Your Name]

P.S. I discovered ${celebrity.name} through CelebrityFanCard and I'm now following them! ğŸŒŸ`);
      
      const mailtoLink = `mailto:fancards@celebrityfancard.com?subject=${subject}&body=${body}`;
      window.open(mailtoLink, '_blank');
    }

    document.getElementById('membershipBtn').addEventListener('click', () => {
      showModal('membershipModal');
    });

    document.getElementById('closeMembership').addEventListener('click', () => {
      hideModal('membershipModal');
    });

    document.getElementById('confirmMembership').addEventListener('click', () => {
      hideModal('membershipModal');
      openEmailDraft();
      showModal('successModal');
    });

    document.getElementById('closeSuccess').addEventListener('click', () => {
      hideModal('successModal');
    });

    document.querySelectorAll('.modal-overlay').forEach(overlay => {
      overlay.addEventListener('click', (e) => {
        if (e.target === overlay) {
          overlay.classList.remove('active');
        }
      });
    });

    async function onConfigChange(config) {
      const customFont = config.font_family || defaultConfig.font_family;
      const baseSize = config.font_size || defaultConfig.font_size;
      const baseFontStack = '-apple-system, BlinkMacSystemFont, "Segoe UI", sans-serif';

      document.body.style.fontFamily = `${customFont}, ${baseFontStack}`;
      document.body.style.background = `linear-gradient(135deg, ${config.background_color || defaultConfig.background_color} 0%, ${config.primary_action_color || defaultConfig.primary_action_color} 100%)`;

      document.getElementById('siteTitle').textContent = config.site_title || defaultConfig.site_title;
      document.getElementById('siteTitle').style.fontSize = `${baseSize * 1.875}px`;
      document.getElementById('siteTitle').style.background = `linear-gradient(135deg, ${config.primary_action_color || defaultConfig.primary_action_color}, ${config.secondary_action_color || defaultConfig.secondary_action_color})`;
      document.getElementById('siteTitle').style.webkitBackgroundClip = 'text';
      document.getElementById('siteTitle').style.webkitTextFillColor = 'transparent';
      document.getElementById('siteTitle').style.backgroundClip = 'text';

      document.getElementById('tagline').textContent = config.tagline || defaultConfig.tagline;
      document.getElementById('tagline').style.fontSize = `${baseSize * 1.2}px`;
      document.getElementById('tagline').style.color = config.text_color || defaultConfig.text_color;

      document.getElementById('discountBadge').textContent = config.discount_text || defaultConfig.discount_text;
      document.getElementById('discountBadge').style.fontSize = `${baseSize * 0.9}px`;

      document.getElementById('membershipBtn').textContent = config.membership_button_text || defaultConfig.membership_button_text;
      document.getElementById('membershipBtn').style.fontSize = `${baseSize * 0.9}px`;
      document.getElementById('membershipBtn').style.background = `linear-gradient(135deg, ${config.secondary_action_color || defaultConfig.secondary_action_color}, ${config.primary_action_color || defaultConfig.primary_action_color})`;

      document.getElementById('categoriesTitle').textContent = config.categories_title || defaultConfig.categories_title;
      document.getElementById('categoriesTitle').style.fontSize = `${baseSize * 2.5}px`;

      document.querySelectorAll('.header, .search-box, .category-card').forEach(el => {
        el.style.background = `rgba(255, 255, 255, 0.95)`;
      });

      document.querySelectorAll('.category-name, .celebrity-name').forEach(el => {
        el.style.color = config.text_color || defaultConfig.text_color;
      });

      document.querySelectorAll('.search-input').forEach(el => {
        el.style.color = config.text_color || defaultConfig.text_color;
      });

      document.querySelectorAll('.btn-follow').forEach(el => {
        if (!el.classList.contains('following')) {
          el.style.background = `linear-gradient(135deg, ${config.primary_action_color || defaultConfig.primary_action_color}, ${config.secondary_action_color || defaultConfig.secondary_action_color})`;
        }
      });
    }

    if (window.elementSdk) {
      window.elementSdk.init({
        defaultConfig,
        onConfigChange,
        mapToCapabilities: (config) => ({
          recolorables: [
            {
              get: () => config.background_color || defaultConfig.background_color,
              set: (value) => {
                config.background_color = value;
                window.elementSdk.setConfig({ background_color: value });
              }
            },
            {
              get: () => config.surface_color || defaultConfig.surface_color,
              set: (value) => {
                config.surface_color = value;
                window.elementSdk.setConfig({ surface_color: value });
              }
            },
            {
              get: () => config.text_color || defaultConfig.text_color,
              set: (value) => {
                config.text_color = value;
                window.elementSdk.setConfig({ text_color: value });
              }
            },
            {
              get: () => config.primary_action_color || defaultConfig.primary_action_color,
              set: (value) => {
                config.primary_action_color = value;
                window.elementSdk.setConfig({ primary_action_color: value });
              }
            },
            {
              get: () => config.secondary_action_color || defaultConfig.secondary_action_color,
              set: (value) => {
                config.secondary_action_color = value;
                window.elementSdk.setConfig({ secondary_action_color: value });
              }
            }
          ],
          borderables: [],
          fontEditable: {
            get: () => config.font_family || defaultConfig.font_family,
            set: (value) => {
              config.font_family = value;
              window.elementSdk.setConfig({ font_family: value });
            }
          },
          fontSizeable: {
            get: () => config.font_size || defaultConfig.font_size,
            set: (value) => {
              config.font_size = value;
              window.elementSdk.setConfig({ font_size: value });
            }
          }
        }),
        mapToEditPanelValues: (config) => new Map([
          ["site_title", config.site_title || defaultConfig.site_title],
          ["tagline", config.tagline || defaultConfig.tagline],
          ["discount_text", config.discount_text || defaultConfig.discount_text],
          ["membership_button_text", config.membership_button_text || defaultConfig.membership_button_text],
          ["categories_title", config.categories_title || defaultConfig.categories_title]
        ])
      });
    }

    setupSearch();
    renderCategories();
    renderCelebrities();
  </script>
